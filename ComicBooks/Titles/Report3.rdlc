      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetDataNumOwned</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>ComicBookDataSet</rd:DataSetName>
        <rd:TableAdapterFillMethod>FillNumOwned</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>ComicBookNumOwnedTableAdapter</rd:TableAdapterName>
        <rd:TableName>ComicBookNumOwned</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT Title, Count(DISTINCT IssueNum) AS 'Count of Owned'
FROM ComicBookDetails 
WHERE (Own = 'True')
GROUP BY Title</CommandText>
        <DataSourceName>ComicBookConnectionString</DataSourceName>
      </Query>
